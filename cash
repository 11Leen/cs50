#include <stdio.h>
#include <cs50.h>
int main(void)

{
    int change;
    int coins = 0;

    do
    {
        printf("change : ");
        scanf("%d", &change);
    }
    while (change <= 0);
    while (change >= 25)
    {
        change = change - 25;
        coins++;
    }
    while (change >= 10)
    {
        change = change - 10;
        coins++;
    }
    while (change >= 5)
    {
        change = change - 5;
        coins++;
    }
    while (change >= 1)
    {
        change = change -1;
        coins++;
    }
    printf("%d\n", coins);
    return 0;

}
